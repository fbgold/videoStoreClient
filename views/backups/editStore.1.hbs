
<div class="container">
    <form action="/video/" method="POST" autocomplete="off">
        {{#with storeData as |store|}}
            <div class="row justify-content-center">
                <div class="col-sm-1">
                    <label>Store Id</label>
                    <input type="text" class="form-control" name="storeId" value="{{store.store_id}}" disabled>
                    <input type="hidden" class="form-control" name="Id" value="{{store.store_id}}" >
                </div>
            </div>
            <br>
            <div class="row justify-content-center">
                {{#with store.Address as |address|}}
                <div class="col-sm-3">
                    <label>Address</label>
                    <input type="text" class="form-control" name="address" value="{{address.address}}">
                </div>
                <div class="col-sm-2">
                    <label>City</label>
                    <input type="text" class="form-control" name="city" value="{{address.City.city}}">
                </div>
                <div class="col-sm-2">
                    <label>State</label>
                    <input type="text" class="form-control" name="state" value="{{address.district}}">
                </div>
                <div class="col-sm-1">
                    <label>Zip</label>
                    <input type="text" class="form-control" name="zip" value="{{address.postal_code}}">
                </div>
                <div class="col-sm-2">
                    <label>Country</label>
                    <input type="text" class="form-control" name="country" value="{{address.City.Country.country}}">
                </div>
                <div class="col-sm-2">
                    <label>Phone</label>
                    <input type="text" class="form-control" name="phone" value="{{address.phone}}">
                </div>
                {{/with}}
            </div>
        {{/with}}
        <br>
        <div class="container">
            <div class="row">
                <button type="submit" class="btn btn-info"><i class="fa fa-database"></i> Submit</button>
            </div>
        </div>
    </form>
</div>